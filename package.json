{
  "name": "livesight",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "description": "See Beyond Barriers - AI-powered navigation assistant empowering visually impaired users to explore the world with confidence",
  "author": "LiveSight Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "build:mobile": "npm run build && npx cap sync",
    "android": "npx cap open android",
    "android:build": "npm run build:mobile && cd android && ./gradlew assembleDebug",
    "android:run": "npm run build:mobile && npx cap run android",
    "ios": "npx cap open ios",
    "ios:run": "npm run build:mobile && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/core": "^8.0.1",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/share": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@google/genai": "^1.40.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.5",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
